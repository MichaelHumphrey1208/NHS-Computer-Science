    toggels(B12)
Bl0 = tkinter.Button(top, text ="Lv0", bg = "#808080", command = level0callback, height=4, width=4)
Bl1 = tkinter.Button(top, text ="Lv1", bg = "#808080", command = level1callback, height=4, width=4)
Bl2 = tkinter.Button(top, text ="Lv2", bg = "#808080", command = level2callback, height=4, width=4)
Bl3 = tkinter.Button(top, text ="Lv3", bg = "#808080", command = level3callback, height=4, width=4)
Bl4 = tkinter.Button(top, text ="Lv4", bg = "#808080", command = level4callback, height=4, width=4)
B1 = tkinter.Button(top, text ="1",    bg = "#808080", command = oneCallback, height=4, width=4)
B2 = tkinter.Button(top, text ="2",    bg = "#808080", command = twoCallback, height=4, width=4)
