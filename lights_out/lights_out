import tkinter

top = tkinter.Tk()

top.geometry('201x341')
top.title("lights out")

def toggels(B):
    if B.cget('bg') == '#FFFF00':
        B['bg'] = '#808080'
    else:
        B['bg'] = '#FFFF00'
def clear(B):
    if B.cget('bg') == '#FFFF00':
        B['bg'] = '#808080'
def level0callback():
    print("level zero")
    clear(B1)
    clear(B2)
    clear(B3)
    clear(B4)
    clear(B5)
    clear(B6)
    clear(B7)
    clear(B8)
    clear(B9)
    clear(B10)
    clear(B11)
    clear(B12)
    clear(B13)
    clear(B14)
    clear(B15)
    clear(B16)
    toggels(B1)
    toggels(B11)
    toggels(B14)
    toggels(B12)
    toggels(B6)
def level1callback():
    print("level one")
    clear(B1)
    clear(B2)
    clear(B3)
    clear(B4)
    clear(B5)
    clear(B6)
    clear(B7)
    clear(B8)
    clear(B9)
    clear(B10)
    clear(B11)
    clear(B12)
    clear(B13)
    clear(B14)
    clear(B15)
    clear(B16)
    toggels(B7)
    toggels(B10)
    toggels(B12)
    toggels(B11)
    toggels(B9)
    toggels(B2)
